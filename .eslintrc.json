{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "extends": [
        "airbnb",
        "plugin:meteor/recommended"
    ],
    "plugins": [
        "meteor",
        "react",
        "jsx-a11y",
        "import"
    ]
}
